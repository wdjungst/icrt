%html
  %head
    %link{:href => "//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.1/css/bootstrap-combined.no-icons.min.css", :rel => "stylesheet"}/
    %link{:href => "//netdna.bootstrapcdn.com/font-awesome/3.1.1/css/font-awesome.css", :rel => "stylesheet"}/
    %link{:href => "/css/icrt.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "/css/bootstrap.min.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "/css/bootstrap-responsive.min.css", :rel => "stylesheet", :type => "text/css"}/
    %meta{:charset => "utf-8"}
    %meta{:content => "IE=edge,chrome=1", "http-equiv" => "X-UA'Compatible"}/
    %title
      Instructure Conference Room Tracker
    %meta{:content => "", :name => "description"}/
    %meta{:content => "width=device-width", :name => "viewport"}/
    :css
      body {
        padding-top: 60px;
        padding-bottom: 40px;
      }
  %body
    .container
      .page-header.title.text-center
        %h1#duration
        %label#settings.btn
          Settings
          %i.icon-cogs
        #time_frame.hide
          %label{:class => 'help-inline', :for => 'time_select'} I need a room for:
          %select#time_select
            %option{:value => '30'}
              %p 30 Minutes
            %option{:value => '60'}
              %p 1 Hour
            %option{:value => '90'}
              %p 1.5 Hours
            %option{:value => '120'}
              %p 2 Hours
            %option{:value => '150'}
              %p 2.5 Hours
            %option{:value => '180'}
              %p 3 Hours
            %option{:value => '210'}
              %p 3.5 Hours
            %option{:value => '240'}
              %p 4 Hours
      %img{:class => "map", :border => "0", :height => "365", :src => "images/conference_rooms.png", :usemap => "#map", :width => "756"}/
